{
    "name": "owenvoke/ecologi-cli",
    "description": "A demo CLI tool for Ecologi based on Laravel Zero.",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.5",
        "illuminate/http": "^10.0",
        "laravel-zero/framework": "^10.0"
    },
    "require-dev": {
        "laravel/pint": "^1.9",
        "mockery/mockery": "^1.4.2",
        "pestphp/pest": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["ecologi"]
}
